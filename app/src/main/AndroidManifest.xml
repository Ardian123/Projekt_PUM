<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ardian.menuexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.kuznargroup.welowanie.QuestionActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.Design.NoActionBar">

        </activity>
        <activity
            android:name="com.example.kuznargroup.welowanie.LoginActivity"
            android:label="WELowanie"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.Design.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kuznargroup.welowanie.MenuActivity"
            android:label="Login"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.Design.NoActionBar"></activity>
        <activity
            android:name="com.example.kuznargroup.welowanie.SelectStageActivity"
            android:label="Login"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.Design.NoActionBar"></activity>
    </application>

</manifest>
